{% extends 'base.html.twig' %}

{% block title %}My Blog{% endblock %}

{% block body %}
    <h1>Статьи</h1>


    {% for post in posts %}
        {#<p>{{ post.createdAt }}</p>#}
        <h2>{{ post.title }}</h2>
        <p>{{ post.description }}</p>
        <a href="{{ path('post',{'id': post.id}) }}">Читать далее...</a>
        <hr>
    {% endfor %}

{% endblock %}
